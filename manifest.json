{
	"manifest_version": 2,
	"name": "Image to EPUB3",
	"version": "1.0.0",
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "manga.png",
		"default_title": "EPUB作成"
	},
	"options_ui": {
		"page": "index.html",
		"chrome_style": false
		,"open_in_tab": true
	},
	"description": "固定レイアウトのEPUBを作るクローム拡張",
	  "icons": {
      "128": "manga.png"
   },
   "short_name": "FLEpub3JS",
	"permissions": [
	"activeTab"
	]
}
